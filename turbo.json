{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": [
				"^build",
				"$NODE_ENV",
				"$PORT"
			],
			"outputs": [
				".next/**",
				"!.next/cache/**"
			]
		},
		"lint": {
			"outputs": []
		},
		"typecheck": {
			"outputs": []
		},
		"dev": {
			"cache": false
		}
	}
}